<h1>Listing laptop_histories</h1>

<table>
  <thead>
    <tr>
      <th>Laptop</th>
      <th>Transaction</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @laptop_histories.each do |laptop_history| %>
      <tr>
        <td><%= laptop_history.laptop_id %></td>
        <td><%= laptop_history.transaction_id %></td>
        <td><%= link_to 'Show', laptop_history %></td>
        <td><%= link_to 'Edit', edit_laptop_history_path(laptop_history) %></td>
        <td><%= link_to 'Destroy', laptop_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

